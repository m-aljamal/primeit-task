// This is your Prisma schema file,

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String  @id @default(cuid())
  name     String
  email    String
  password String
  avatar   String
  eduType  String?
}
